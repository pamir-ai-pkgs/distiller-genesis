# Pre-removal script for distiller-genesis
# Meta-package - minimal pre-removal tasks

case "$1" in
    remove|upgrade|deconfigure)
        # Nothing to do for meta-package
        :
        ;;

    failed-upgrade)
        ;;

    *)
        echo "prerm called with unknown argument \`$1'" >&2
        exit 1
        ;;
esac

#DEBHELPER#

